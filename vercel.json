      "destination": "/api/moxproxy?path=$1"
    },
    {
      "source": "/api/archidekt/(.*)",
      "destination": "/api/archproxy?path=$1"
    },
    {
      "source": "/archidekt/(.*)",
      "destination": "/api/archproxy?path=$1"
    },
    {
      "source": "/api/topdeck/(.*)",
      "destination": "/api/topdeckproxy?path=$1"
    },
    {
      "source": "/topdeck/(.*)",
      "destination": "/api/topdeckproxy?path=$1"
    }
  ]
